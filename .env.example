# Application Environment
NODE_ENV=development
PORT=3000
HOST=0.0.0.0

# PostgreSQL Database
# Note: Port 5434 is used to avoid conflicts with other PostgreSQL instances
DATABASE_URL=postgresql://buildinglens_user:buildinglens_pass@localhost:5434/buildinglens

# Redis Cache
# Note: Port 6380 is used to avoid conflicts with other Redis instances
REDIS_URL=redis://localhost:6380

# Redis TTL (Time To Live) in seconds
REDIS_TTL_GEOCODING=2592000  # 30 days
REDIS_TTL_PLACES=604800      # 7 days
REDIS_TTL_IDENTIFY=3600      # 1 hour

# Google Maps API
GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# Nominatim (OpenStreetMap)
NOMINATIM_BASE_URL=https://nominatim.openstreetmap.org
NOMINATIM_EMAIL=your_email@example.com

# Search Configuration
SEARCH_RADIUS_METERS=100  # Default search radius for nearby places

# Rate Limiting
RATE_LIMIT_WINDOW_MS=60000      # 1 minute
RATE_LIMIT_MAX_REQUESTS=10      # 10 requests per window

# Logging
LOG_LEVEL=debug

# CORS
CORS_ORIGIN=*
